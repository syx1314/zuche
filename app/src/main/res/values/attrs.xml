<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PriceRangeSeekBar">
        <attr name="track_height" format="dimension" />
        <attr name="track_unselected_color" format="reference|color" />
        <attr name="track_selected_color" format="reference|color" />
        <attr name="thumb_drawable" format="reference" />
        <attr name="track_margin" format="dimension" />
        <attr name="label_normal_text_size" format="dimension" />
        <attr name="label_pressed_text_size" format="dimension" />
        <attr name="label_text_color" format="reference|color" />
        <attr name="label_text_margin" format="dimension" />
        <attr name="support_zoom" format="boolean" />
    </declare-styleable>
    <!--cp_common_tap_layout-->
    <declare-styleable name="cp_common_tab_layout">
        <!--当前选中项-->
        <attr name="checked_index" format="integer" />
    </declare-styleable>
    <!--cp_common_header-->
    <declare-styleable name="cp_common_header">
        <!--提交按钮是否显示-->
        <attr name="btn_visibility" format="boolean" />
        <!--是否显示返回按钮-->
        <attr name="btn_return_visibility" format="boolean" />
        <!--提交按钮名称-->
        <attr name="btn_name" format="string" />
        <!--标题-->
        <attr name="header_title" format="string" />
    </declare-styleable>
    <!--cp_common_header_Image-->
    <declare-styleable name="cp_common_header_image">
        <!--提交按钮是否显示-->
        <attr name="right_btn_visibility" format="boolean" />
        <!--提交按钮名称-->
        <attr name="right_btn_ico" format="reference" />
        <!--标题-->
        <attr name="right_header_title" format="string" />
    </declare-styleable>
    <!--view_cp_common_number-->
    <declare-styleable name="cp_common_number">
        <!--最小-->
        <attr name="min" format="integer" />
        <!--最大-->
        <attr name="max" format="integer" />
        <!--是否可输入-->
        <attr name="enable" format="boolean" />
    </declare-styleable>
    <declare-styleable name="cp_main_header">
        <!--左侧按钮是否显示自定义的按钮-->
        <attr name="is_btn" format="boolean" />
        <!--右边图标-->
        <attr name="right_ico" format="reference" />
    </declare-styleable>
    <!--view_common_query_bottom_btn-->
    <declare-styleable name="cp_common_query_bottom">
        <attr name="is_flight" format="boolean" />
        <attr name="is_hotel" format="boolean" />
        <attr name="is_train" format="boolean" />
        <attr name="is_car" format="boolean" />
        <!--是否企业-->
        <attr name="is_com" format="boolean" />
    </declare-styleable>
    <!--酒店热门城市-->

    <string-array name="hotel_city">
        <item>北京</item>
        <item>上海</item>
        <item>广州</item>
        <item>深圳</item>
        <item>天津</item>
        <item>重庆</item>
        <item>武汉</item>
        <item>三亚</item>
        <item>成都</item>
        <item>昆明</item>
        <item>西安</item>
        <item>杭州</item>
        <item>苏州</item>
        <item>大连</item>
        <item>青岛</item>
        <item>哈尔滨</item>
        <item>沈阳</item>
    </string-array>
    <!--机票热门城市-->
    <string-array name="flight_city">
        <item>北京</item>
        <item>上海</item>
        <item>广州</item>
        <item>深圳</item>
        <item>天津</item>
        <item>重庆</item>
        <item>武汉</item>
        <item>三亚</item>
        <item>成都</item>
        <item>昆明</item>
        <item>西安</item>
        <item>杭州</item>
        <item>苏州</item>
        <item>大连</item>
        <item>青岛</item>
        <item>哈尔滨</item>
        <item>沈阳</item>
    </string-array>
    <declare-styleable name="SliderLayout">
        <attr name="slider_pic" format="reference" />
    </declare-styleable>

    <declare-styleable name="BottomServiceView">
        <attr name="flag" format="string" />
    </declare-styleable>

    <declare-styleable name="common_title_view">
        <attr name="rightVisible" format="boolean"></attr>
        <attr name="title" format="string"></attr>
        <attr name="right_title" format="string"></attr>
        <attr name="right_title_color" format="reference"></attr>
        <attr name="left_title" format="string"></attr>
        <attr name="left_img" format="reference"></attr>
        <attr name="left_visable" format="string"></attr>
    </declare-styleable>

    <declare-styleable name="RangeSeekBar">
        <attr name="range_max" format="float" />
        <attr name="range_min" format="float" />
        <attr name="reserve" format="float" />
        <attr name="cells" format="integer" />
        <attr name="lineColorSelected" format="color" />
        <attr name="lineColorEdge" format="color" />
        <attr name="seekBarResId" format="reference" />
    </declare-styleable>

    <declare-styleable name="NumberButton">
        <attr name="editable" format="boolean"/>
        <attr name="buttonWidth" format="dimension" />
        <attr name="textWidth" format="dimension" />
        <attr name="textSize" format="dimension"/>
        <attr name="textColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="AutoScaleTextView">
        <attr name="minTextSize" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="ExpandableTextView">
        <attr name="etv_MaxLinesOnShrink" format="reference|integer" />
        <attr name="etv_EllipsisHint" format="reference|string" /><!--default is ..-->
        <attr name="etv_ToExpandHint" format="reference|string" /><!--"to expand" hint string, default is "Expand"-->
        <attr name="etv_ToShrinkHint" format="reference|string" /><!--"to shrink" hint string, default is "Shrink"-->
        <attr name="etv_GapToExpandHint" format="reference|string" /><!--gap between "toExpandHint" and "trimmed text"-->
        <attr name="etv_GapToShrinkHint" format="reference|string" /><!--gap between "toShrinkHint" and "trimmed text"-->
        <attr name="etv_EnableToggle" format="reference|boolean" /><!--if toggle the state when click ExpandTextView-->
        <attr name="etv_ToExpandHintShow" format="reference|boolean" /><!--if show "toExpandHint"-->
        <attr name="etv_ToShrinkHintShow" format="reference|boolean" /><!--if show "toShrinkHint"-->
        <attr name="etv_ToExpandHintColor" format="reference|color" /><!--text color of "toExpandHint"-->
        <attr name="etv_ToShrinkHintColor" format="reference|color" /><!--text color of "toShrinkHint"-->
        <attr name="etv_ToExpandHintColorBgPressed" format="reference|color" /><!--background color if "toExpandHint" pressed-->
        <attr name="etv_ToShrinkHintColorBgPressed" format="reference|color" /><!--background color if "toShrinkHint" pressed-->
        <attr name="etv_InitState" format="enum"><!--init state, default is shrink-->
            <enum name="shrink" value="0"/>
            <enum name="expand" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="EnhanceTabLayout">
        <attr name="tab_Mode" format="enum">
            <enum name="mode_fixed" value="1"/>
            <enum name="mode_scrollable" value="2"/>
        </attr>
        <attr name="tabIndicatorColor" format="color"/>
        <attr name="tabSelectTextColor" format="color"/>
        <attr name="tabTextColor" format="color"/>
        <attr name="tabIndicatorHeight" format="dimension"/>
        <attr name="tabIndicatorWidth" format="dimension"/>
        <attr name="tabTextSize" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="MyImageView">
        <attr name="my_drawable" format="reference"></attr>
        <attr name="start_color" format="color"></attr>
        <attr name="end_color" format="color"></attr>
    </declare-styleable>
    <declare-styleable name="MoreCircleView">
        <attr name="backWidth" format="dimension" />    <!--背景圆环宽度-->
        <attr name="progWidth" format="dimension" />    <!--进度圆环宽度-->
        <attr name="backColor" format="color" />        <!--背景圆环颜色-->
        <attr name="progColor" format="color" />        <!--进度圆环颜色-->
        <attr name="progStartColor" format="color" />   <!--进度圆环开始颜色-->
        <attr name="progFirstColor" format="color" />   <!--进度圆环结束颜色-->
        <attr name="progress" format="integer" />       <!--圆环进度-->
        <attr name="sweepAngle" format="float" />       <!--圆环进度-->
    </declare-styleable>

</resources>